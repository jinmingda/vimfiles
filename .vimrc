execute pathogen#infect()

set nocompatible

colorscheme gruvbox

set guifont=Consolas:h11:cANSI

filetype plugin on
filetype indent on

syntax on

set expandtab

set autoindent
set smartindent

set tabstop=4

set softtabstop=4

set shiftwidth=4

set backspace=indent,eol,start

set wrap

set number

set ignorecase
set smartcase

set ruler

set wildmenu

set guioptions-=T

set incsearch
set hlsearch

set noswapfile


set encoding=utf-8

set clipboard=unnamed

set nofoldenable

set splitright

nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
